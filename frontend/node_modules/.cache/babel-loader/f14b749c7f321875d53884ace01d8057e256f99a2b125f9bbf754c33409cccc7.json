{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      user: {\n        username: '',\n        email: '',\n        password: ''\n      },\n      message: ''\n    };\n  },\n  methods: {\n    async registerUser() {\n      try {\n        const response = await axios.post('/api/register', this.user);\n        console.log('Utente registrato:', response.data);\n        this.message = 'Registrazione avvenuta con successo!';\n        this.$router.push('/');\n        this.user = {\n          username: '',\n          email: '',\n          password: ''\n        };\n      } catch (error) {\n        if (error.response && error.response.status === 400) {\n          this.message = 'Nome utente già presente. Scegli un altro nome utente.';\n        } else {\n          this.message = 'Errore durante la registrazione.';\n        }\n        console.error('Errore:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","user","username","email","password","message","methods","registerUser","response","post","console","log","$router","push","error","status"],"sources":["/home/livia/Scrivania/TW/progettoTWvue/frontend/src/views/registratiAccedi.vue"],"sourcesContent":["<template>\n  <div class=\"registration\">\n    <h1 class=\"registration__header\">SELFIE</h1> \n    <div class=\"registration__container\">\n      <img src=\"@/assets/images/logo.png\" alt=\"Logo\" class=\"registration__logo\" />\n      <h1 class=\"registration__title\">Registrati</h1>\n      <form @submit.prevent=\"registerUser\">\n        <div class=\"registration__field\">\n          <i class=\"registration__icon fas fa-user\"></i>\n          <input v-model=\"user.username\" id=\"username\" class=\"registration__input\" placeholder=\"Nome Utente\" required />\n        </div>\n        <div class=\"registration__field\">\n          <i class=\"registration__icon fas fa-envelope\"></i>\n          <input v-model=\"user.email\" id=\"email\" class=\"registration__input\" placeholder=\"Email\" type=\"email\" required />\n        </div>\n        <div class=\"registration__field\">\n          <i class=\"registration__icon fas fa-lock\"></i>\n          <input v-model=\"user.password\" type=\"password\" id=\"password\" class=\"registration__input\" placeholder=\"Password\" required />\n        </div>\n        <button class=\"registration__submit\" type=\"submit\">Registrati</button>\n        <p v-if=\"message\" class=\"message\">{{ message }}</p>\n      </form>\n      <a class=\"registration__link\" href=\"/\">Torna alla Login</a>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      user: {\n        username: '',\n        email: '',      \n        password: ''\n      },\n      message: ''\n    };\n  },\n  methods: {\n    async registerUser() {\n      try {\n        const response = await axios.post('/api/register', this.user);\n        console.log('Utente registrato:', response.data);\n        this.message = 'Registrazione avvenuta con successo!';\n        this.$router.push('/');\n        this.user = {\n          username: '',\n          email: '',       \n          password: ''\n        };\n      } catch (error) {\n        if (error.response && error.response.status === 400) {\n          this.message = 'Nome utente già presente. Scegli un altro nome utente.';\n        } else {\n          this.message = 'Errore durante la registrazione.';\n        }\n        console.error('Errore:', error);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.registration {\n  background: linear-gradient(to bottom, #f4a460, #eee8aa);\n  display: flex;\n  flex-direction: column; \n  align-items: center;\n  justify-content: center;\n  min-height: 100vh;\n}\n\n.registration__header {\n  text-align: center; \n  color: #fff;\n  font-size: 48px;\n  margin-bottom: 20px; \n}\n\n.registration__container {\n  background: linear-gradient(to bottom, #f4a460, #eee8aa); \n  border-radius: 20px;\n  box-shadow: 0px 0px 24px #5C5696;\n  width: 360px;\n  padding: 30px;\n  position: relative;\n}\n\n.registration__logo {\n  display: block;\n  margin: 0 auto 20px; \n  max-width: 100%; \n  height: auto;\n}\n\n.registration__title {\n  text-align: center;\n  color: #fff;\n  margin-bottom: 20px;\n}\n\n.registration__field {\n  margin-bottom: 20px;\n  position: relative;\n}\n\n.registration__icon {\n  position: absolute;\n  top: 15px;\n  color: #7875B5;\n}\n\n.registration__input {\n  border: none;\n  border-bottom: 2px solid #D1D1D4;\n  background: none;\n  padding: 10px;\n  padding-left: 30px;\n  font-weight: 700;\n  width: 100%;\n  transition: .2s;\n}\n\n.registration__input:focus,\n.registration__input:hover {\n  outline: none;\n  border-bottom-color: #6A679E;\n}\n\n.registration__submit {\n  background: #fff;\n  font-size: 14px;\n  padding: 16px 20px;\n  border-radius: 26px;\n  border: 1px solid #D4D3E8;\n  text-transform: uppercase;\n  font-weight: 700;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 100%;\n  color: #4C489D;\n  box-shadow: 0px 2px 2px #5C5696;\n  cursor: pointer;\n  transition: .2s;\n}\n\n.registration__submit:focus,\n.registration__submit:hover {\n  border-color: #6A679E;\n  outline: none;\n}\n\n.message {\n  color: black;\n  text-align: center;\n  margin-top: 10px;\n}\n\n.registration__link {\n  display: block;\n  text-align: center;\n  color: #f4a460;\n  text-decoration: none;\n  font-weight: 700;\n  margin-top: 20px;\n  cursor: pointer;\n}\n</style>\n"],"mappings":";AA4BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACZ,CAAC;MACDC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,IAAI,CAAC,eAAe,EAAE,IAAI,CAACR,IAAI,CAAC;QAC7DS,OAAO,CAACC,GAAG,CAAC,oBAAoB,EAAEH,QAAQ,CAACR,IAAI,CAAC;QAChD,